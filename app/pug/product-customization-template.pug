doctype html
html(lang="en")
    head
        meta(charset="utf-8")
        meta(http-equiv='X-UA-Compatible', content="IE = edge")
        meta(name="viewport" content="width=device-width,initial-scale=1, maximum-scale=1")
        meta(property="og:image" content="path/to/image.jpg")
        meta(name="keywords" content="")
        link(rel="icon" href="img/favicon/favicon.ico")
        link(rel="apple-touch-icon" sizes="180x180" href="img/favicon/apple-touch-icon-180x180.png")
        meta(name="theme-color" content="#000")
        link(rel="stylesheet" type='text/css' href="libs/mmenu-light/dist/mmenu-light.css")
        link(rel="stylesheet" type='text/css' href="libs/mmenu-js/dist/mmenu.css")
        link(rel="stylesheet" type='text/css' href="https://unpkg.com/swiper@7/swiper-bundle.min.css")
        link(rel="stylesheet" type='text/css' href="https://cdnjs.cloudflare.com/ajax/libs/lightgallery-js/1.4.0/css/lightgallery.min.css")
        link(rel="stylesheet" type='text/css' href="css/main.min.css")
        title
            block title
    body
        <!--HEADER-->
        include components/_mixin
        <!--HEADER END-->
        <!--CONTENT-->
        block content
        <!--CONTENT END-->
        <!--FOOTER-->
        include layout/footer
        <!--FOOTER END-->
        <!--MODAL-->
        include modal/_modal-subscribe
        include modal/_modal-mainimage-popup
        include modal/_modal-customize-popup-1
        include modal/_modal-customize-popup-5
        include modal/_modal-customize-popup-7
        include modal/_modal-customize-popup-8
        include modal/_modal-customize-popup-9
        include modal/_modal-customize-popup-10
        include modal/_modal-customize-fullsize
        <!--MODAL END-->
        script(src="js/jquery.js")
        script(src="js/libs.js")
        script(src="libs/mmenu-js/dist/mmenu.polyfills.js")
        script(src="libs/mmenu-js/dist/mmenu.js")
        script(src="libs/mmenu-light/dist/mmenu-light.js")
        script(src="libs/slick/slick.min.js")
        script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js")
        script(src="https://cdn.jsdelivr.net/npm/bootstrap-select@1.14.0-beta2/dist/js/bootstrap-select.min.js")
        script(src="https://unpkg.com/swiper@7/swiper-bundle.min.js")
        script(src="https://cdnjs.cloudflare.com/ajax/libs/lightgallery-js/1.4.0/js/lightgallery.min.js")
        script(src="js/main.js")
