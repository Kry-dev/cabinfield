extends ../main-templateblock title    | Kennet Seriesblock content    +breadcrumbs([['Bedrooms', '#'], ['Shop Style', '#'], ['Shop Collection', '#'], ['Product Listing', '#']])    main.content.pt-2        .container            .row                .col-md-12.col-lg-5.col-xl-6                    h1.mb-md-3 Kennet Series                .col-lg-7.col-xl-6.d-none.d-lg-inline-flex.justify-content-end                    .product-collection-sort                        label.mb-0() Sort by:                        select.selectpicker.form-control.mx-2                            option(selected='') High to low                            option(value='1') Low to high                            option(value='2') Two                            option(value='3') Three                        p.mb-0                            a.btn.btn-warning(href='#') Shop by furniture piece            //DETAILS MOBILE FILTER BUTTONS            .row.d-flex.d-lg-none                .col-md-4.col-xs-12.mb-4.mb-md-0                    a.btn.btn-outline-primary.w-100(href="#") Shop by furniture piece                .col-md-4.col-6                    button.btn.btn-outline-warning.w-100( onclick="$('#mobile-sort').show();") Sort                .col-md-4.col-6                    button.btn.btn-warning.w-100( onclick="$('#mobile-filter-list').show();") Filter Search            //mobile filter list            .filter-mobile(id="mobile-filter-list")                .filter-mobile--top                    button.text-primary.text-uppercase(onclick="$('#mobile-filter-list').hide();") #[i.fa.fa-caret-left.text-black.me-3] Go back                .row.filter-mobile--cta                    .col-6                        a.btn.btn-outline-warning.w-100(href="#") Clear                    .col-6                        a.btn.btn-warning.w-100(href="#") Filter Search                .filter-tabs                    .filter-tab                        input#chck1(type='checkbox')                        label.filter-tab--label(for='chck1') Furniture Type                            span.text-bold (4)                        .filter-tab--content                            ul.product-filters-list                                li.checkbox.custom_checkbox                                    input#fur-type-0(type='checkbox', name='', value='')                                    label(for='fur-type-0') Bedroom Furniture set                                li.checkbox.custom_checkbox                                    input#fur-type-1(type='checkbox', name='', value='', checked='')                                    label(for='fur-type-1') Beds and Headboards                                li.checkbox.custom_checkbox                                    input#fur-type-2(type='checkbox', name='', value='')                                    label(for='fur-type-2') Bedroom Storage Pieces                                li.checkbox.custom_checkbox                                    input#fur-type-3(type='checkbox', name='', value='')                                    label(for='fur-type-3') Bedroom Accessories                    .filter-tab                        input#chck2(type='checkbox')                        label.filter-tab--label(for='chck2') Collection                            span.text-bold (12)                        .filter-tab--content                            ul.product-filters-list                                li.checkbox.custom_checkbox                                    input#wood-mob-0(type='checkbox', name='Wood', value='')                                    label(for='wood-mob-0') Oak                                li.checkbox.custom_checkbox                                    input#wood-mob-1(type='checkbox', name='Wood', value='')                                    label(for='wood-mob-1') Cherry                                li.checkbox.custom_checkbox                                    input#wood-mob-2(type='checkbox', name='Wood', value='')                                    label(for='wood-mob-2') Brown Maple                                li.checkbox.custom_checkbox                                    input#wood-mob-3(type='checkbox', name='Wood', value='')                                    label(for='wood-mob-3') Quartersawn Oak                                li.checkbox.custom_checkbox                                    input#wood-mob-4(type='checkbox', name='Wood', value='')                                    label(for='wood-mob-4') Bearwood-mob-                                li.checkbox.custom_checkbox                                    input#wood-mob-5(type='checkbox', name='Wood', value='')                                    label(for='wood-mob-5') Apache                                li.checkbox.custom_checkbox                                    input#wood-mob-6(type='checkbox', name='Wood', value='')                                    label(for='wood-mob-6') Hard Maple                                li.checkbox.custom_checkbox                                    input#wood-mob-7(type='checkbox', name='Wood', value='')                                    label(for='wood-mob-7') Walnut                                li.checkbox.custom_checkbox                                    input#wood-mob-8(type='checkbox', name='Wood', value='')                                    label(for='wood-mob-8') Hickory                                li.checkbox.custom_checkbox                                    input#wood-mob-9(type='checkbox', name='Wood', value='')                                    label(for='wood-mob-9') Hard Maple                                li.checkbox.custom_checkbox                                    input#wood-mob-10(type='checkbox', name='Wood', value='')                                    label(for='wood-mob-10') Rustic Hickory                                li.checkbox.custom_checkbox                                    input#wood-mob-11(type='checkbox', name='Wood', value='')                                    label(for='wood-mob-11') Hickory Twigs & Oak                                li.checkbox.custom_checkbox                                    input#wood-mob-12(type='checkbox', name='Wood', value='')                                    label(for='wood-mob-12') Hickory Twigs & Rustic Alder                    .filter-tab                        input#chck3(type='checkbox')                        label.filter-tab--label(for='chck3') Style                            span.text-bold (7)                        .filter-tab--content                            ul.product-filters-list                                li.checkbox.custom_checkbox                                    input#style-mob-0(type='checkbox', name='', value='')                                    label(for='style-mob-0') Contemporary                                li.checkbox.custom_checkbox                                    input#style-mob-1(type='checkbox', name='', value='', checked='')                                    label(for='style-mob-1') Mission/Craftsman/Arts & Crafts                                li.checkbox.custom_checkbox                                    input#style-mob-2(type='checkbox', name='', value='')                                    label(for='style-mob-2') Mission Beds and Headboards                                li.checkbox.custom_checkbox                                    input#style-mob-3(type='checkbox', name='', value='')                                    label(for='style-mob-3') Traditional                                li.checkbox.custom_checkbox                                    input#style-mob-4(type='checkbox', name='', value='')                                    label(for='style-mob-4') Eclectic                                li.checkbox.custom_checkbox                                    input#style-mob-5(type='checkbox', name='', value='')                                    label(for='style-mob-5') Economy/Value                                li.checkbox.custom_checkbox                                    input#style-mob-6(type='checkbox', name='', value='')                                    label(for='style-mob-6') Traditional            //mobile filter sort            .filter-mobile(id="mobile-sort")                .filter-mobile--top                    button.text-primary.text-uppercase(onclick="$('#mobile-sort').hide();") #[i.fa.fa-caret-left.text-black.me-3] Go back                .row.filter-mobile--cta                    .col-6                        a.btn.btn-outline-warning.w-100(href="#") Clear                    .col-6                        a.btn.btn-warning.w-100(href="#") Filter Search                .filter-tabs                    ul.product-filters-list                        li.checkbox.custom_checkbox                            input#sort-0(type='checkbox', name='', value='')                            label(for='sort-0') Low to high                        li.checkbox.custom_checkbox                            input#sort-1(type='checkbox', name='', value='', checked='')                            label(for='sort-1') High to low                        li.checkbox.custom_checkbox                            input#sort-2(type='checkbox', name='', value='')                            label(for='sort-2') Most Popular                        li.checkbox.custom_checkbox                            input#sort-3(type='checkbox', name='', value='')                            label(for='sort-3') Most Relevant        <!--Filter product list-->        include ../layout/_product-collection-listing        <!--Filter product list end-->