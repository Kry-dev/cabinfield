.pagination
  .pagination-inner
    a.pagination-arrow.prev.inactive(href='#')
      span Previous Page
    ul
      li #[a.inactive(href='#') 1]
      li #[a(href='#') 2]
      li #[a(href='#') 3]
      li #[a(href='#') 4]
      li #[a(href='#') 5]
      li #[a(href='#') 6]
      li ...
      li #[a(href='#') 21]
    a.pagination-arrow.next(href='#')
      span Next Page
